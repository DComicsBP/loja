@inherits Umbraco.Web.Mvc.UmbracoTemplatePage

<div style=" background-image: url('');"></div>
<nav>

    <div class="nav-wrapper brown darken-1">
       <div style="position: relative; left:50px; "><a href="/" class="brand-logo">Logo</a></div>
        <ul id="nav-mobile" class="right hide-on-med-and-down">

            @{
                var cont = Umbraco.TypedContent(uQuery.GetNodesByType("home").FirstOrDefault().Id);
                var linksHeader = cont.GetPropertyValue<IEnumerable<IPublishedContent>>("linksHeader");

                foreach (var item in linksHeader)
                 {
                         var i = item.GetPropertyValue<string>("title");
                         var link = item.GetPropertyValue<IPublishedContent>("internalLinks").Url.ToString();

                         if (i.Equals("Home")) { continue; }
                         else { <li><a href="@link">@i </a></li> }
                 }
            }

        </ul>



    </div>
    
</nav>



