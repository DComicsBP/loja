@inherits Umbraco.Web.Mvc.UmbracoTemplatePage

<div style=" background-image: url('');"></div>
<nav>

    <div class="nav-wrapper brown darken-1">
        <a href="/" class="brand-logo">Logo</a>
        <ul id="nav-mobile" class="right hide-on-med-and-down">

            @{
                var cont = Umbraco.TypedContent(uQuery.GetNodesByType("home").FirstOrDefault().Id).GetPropertyValue<IEnumerable<IPublishedContent>>("header");
                
                foreach (var con in cont)
                {
                    var title = con.GetPropertyValue<IEnumerable<IPublishedContent>>("links");
                    foreach (var item in title)
                    {

                        var i = item.GetPropertyValue<string>("title");
                        var link = item.GetPropertyValue<IPublishedContent>("internalLinks").Url.ToString();

                        if (i.Equals("Home")) { continue; }
                        else
                        {
                            <li><a href="@link">@i </a></li>
                        }


                    }

                }

            }

        </ul>



    </div>
    
</nav>



