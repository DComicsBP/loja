@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "Master.cshtml";

   

}

@{
    var id = Model.Content.Id;
    switch (id)
    {
        case 1440:
            Html.RenderPartial("~/Views/Partials/Loja/LojaDefault.cshtml");
            break;
        case 1442:
            Html.RenderPartial("~/Views/Partials/Loja/LojaFisica.cshtml");
            break;
        case 1379:
            Html.RenderPartial("~/Views/Partials/Loja/LojaVirtual.cshtml");
            break;

    }


}




    